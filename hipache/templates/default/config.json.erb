{
    "server": {
        "accessLog": "/var/log/hipache_access.log",
        "port": 80,
        "workers": 10,
        "maxSockets": 100,
        "deadBackendTTL": 30,
        "tcpTimeout": 30,
        "errorMaxRetries": 3,
        "https": {
            "port": 443,
            "key": "<%= @key_file %>",
            "cert": "<%= @crt_file %>"
        }
    },
    "redis": {
        "port": 6379,
        "host": "127.0.0.1"
    }
}
